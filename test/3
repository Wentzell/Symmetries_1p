//#include "file.h"
#include <vector>
#include <Eigen/Core>
#include <memory>
#include <iostream>
#include <boost/multi_array.hpp>
#include <chrono>

using namespace std;
using namespace Eigen;

/**
 *	Description of main method	
 */
int main (int argc, char * argv[])
{
   vector<int> a = { 1,2,3,4,5,6 };

   vector<int>::iterator  b = a.begin() + 2;

   cout << " b 0 " << b[0] << endl;
   b[0] = 10;

   cout << " b 0 " << b[0] << endl;
   cout << " a 1 " << a[2] << endl;
   
   return 0;
}


